{
  "env": {
    "browser": true,
    "es2020": true,
    "jest/globals": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "react/jsx-one-expression-per-line": 0,
    "no-unused-vars": 0,
    "react/prop-types": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "**/*/*.spec.js",
          "**/spec_support/*"
        ]
      }
    ]
  }
}